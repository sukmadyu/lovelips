        <script>
            function kriteria(kiri,kanan){
                if (kiri==kanan){
                    return 1;
                }else if (kiri==1){
                    if (kanan==2){
                        return 2;
                    }else if(kanan==3){
                        return 3;
                    }else if(kanan==4){
                        return 4;
                    }else if(kanan==5){
                        return 5;
                    }
                }else if (kiri==2){
                    if (kanan==1){
                        return 0.5;
                    }else if(kanan==3){
                        return 3;
                    }else if(kanan==4){
                        return 4;
                    }else if(kanan==5){
                        return 5;
                    }
                }else if (kiri==3){
                    if (kanan==1){
                        return 0.5;
                    }else if(kanan==2){
<<<<<<< HEAD
